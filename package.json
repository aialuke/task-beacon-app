{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:ui": "vitest --ui --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:run": "vitest run --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:coverage": "vitest run --coverage --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:watch": "vitest --watch --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:unit": "vitest run --dir src --exclude '**/integration/**' --exclude '**/e2e/**' --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:integration": "vitest run src/**/integration/** --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:components": "vitest run src/**/components/**/*.test.* --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:hooks": "vitest run src/**/hooks/**/*.test.* --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:api": "vitest run src/**/api/**/*.test.* --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:critical": "vitest run --testNamePattern='(auth|task.*workflow|user.*flow|critical)' --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml --pool=forks --maxConcurrency=1 --maxWorkers=1",
    "analyze": "knip",
    "analyze:fix": "knip --fix",
    "purge:audit": "postcss src/index.css -o purged.css --config postcss.purge.js && npx stylelint \"src/styles/{architecture,tokens,components,base,utilities}/*.css\" \"src/styles/utilities/{accessibility,animations}/*.css\" --fix --formatter verbose --output-file stylelint-report-final5.json"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-spring/web": "^10.0.1",
    "@supabase/supabase-js": "^2.49.6",
    "@tanstack/react-query": "^5.56.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jsdom": "^26.1.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^3.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@types/node": "^22.5.5",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.2.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "knip": "^5.59.1",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
