    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --dir src --exclude '**/integration/**' --exclude '**/e2e/**'",
    "test:integration": "vitest run src/**/integration/**",
    "test:components": "vitest run src/**/components/**/*.test.*",
    "test:hooks": "vitest run src/**/hooks/**/*.test.*",
    "test:api": "vitest run src/**/api/**/*.test.*",
    "test:critical": "vitest run --testNamePattern='(auth|task.*workflow|user.*flow|critical)'",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "analyze": "knip",
    "analyze:fix": "knip --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:src": "prettier --write src/",
    "purge:audit": "npx stylelint \"src/styles/{architecture,tokens,components,base,utilities}/*.css\" \"src/styles/utilities/{accessibility,animations}/*.css\" --fix --formatter verbose --output-file stylelint-report-final5.json"
  },
